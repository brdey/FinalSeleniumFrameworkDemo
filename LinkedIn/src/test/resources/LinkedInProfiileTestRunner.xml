    <parameter name="cloudEnvName" value="saucelabs"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="firefox"/>
    <parameter name="browserVersion" value="58.0"/>
    <parameter name="url" value="https://www.linkedin.com/"/>
    <test name = "Linked In Profile Test">
        <classes>
            <class name="invalidLoginTest.LoginWithInvalidValue"></class>
           <!-- <class name="AllFeaturesTabsTest.AllFeatureTests"></class>
            <class name="loginPageTest.LoginPageTest"></class>
            <class name="myNetworkTabTest.MyNetworkTabTest"></class>
            <class name="AllFeaturesTabsTest.AllFeatureTests"></class>-->
        </classes>
    </test>
</suite>
