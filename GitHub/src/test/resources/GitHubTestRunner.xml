<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="GitHub" parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="firefox"/>
    <parameter name="browserVersion" value="58.0"/>
    <parameter name="url" value="https://github.com/"/>
    <test name = "Creating Repository in GitHub">
        <classes>
            <class name="gitHubTestCases.GitHubTestCases"></class>
        </classes>
    </test>
</suite>
